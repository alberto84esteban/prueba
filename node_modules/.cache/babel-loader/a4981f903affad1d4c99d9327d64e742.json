{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  components: {},\n\n  setup() {\n    const store = useStore();\n    const menu = ref(false);\n    const title = computed(() => store.state.title);\n\n    const handleClick = () => {\n      store.commit('changeMenuVisibility', !menu.value);\n    };\n\n    return {\n      menu,\n      title,\n      handleClick\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAUA,SAASA,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;AACXC,YAAS,EAAI,EADF;;AAEXC,OAAK,GAAG;AAEN,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,IAAG,GAAIN,GAAG,CAAC,KAAD,CAAhB;AACA,UAAMO,KAAI,GAAIN,QAAQ,CAAC,MAAMI,KAAK,CAACG,KAAN,CAAYD,KAAnB,CAAtB;;AAEA,UAAME,WAAU,GAAI,MAAM;AACxBJ,WAAK,CAACK,MAAN,CAAa,sBAAb,EAAqC,CAAEJ,IAAI,CAACK,KAA5C;AACF,KAFA;;AAIA,WAAO;AAAEL,UAAF;AAAQC,WAAR;AAAeE;AAAf,KAAP;AAED;;AAdU,CAAf","names":["ref","computed","useStore","components","setup","store","menu","title","state","handleClick","commit","value"],"sourceRoot":"","sources":["/Users/alberto/Workspace/atSistemas/prueba/src/components/AppHeader.vue"],"sourcesContent":["<template>\n  <va-app-bar shadow-color='bg-slate-200' class=\"z-[100]\">\n    <va-button icon=\"menu\" color=\"#fff\" flat :rounded=\"false\" @click=\"handleClick\" />\n    <v-spacer />\n    <h3>{{title}}</h3>\n  </va-app-bar>\n</template>\n\n<script>\n\nimport { ref, computed } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default {\n    components : {},\n    setup() {\n\n      const store = useStore();\n      const menu = ref(false);\n      const title = computed(() => store.state.title )\n\n      const handleClick = () => {\n        store.commit('changeMenuVisibility', !(menu.value))\n      }\n\n      return { menu, title, handleClick }\n        \n    },\n}\n</script>\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}