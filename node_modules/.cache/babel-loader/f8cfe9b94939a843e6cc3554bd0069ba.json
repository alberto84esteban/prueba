{"ast":null,"code":"import { computed, ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    const store = useStore();\n    const selected = computed(() => store.state.menuSelected);\n\n    const doClose = () => {\n      store.commit('changeMenuVisibility', false);\n    };\n\n    const doSelect = (id, name) => {\n      console.log(\"doSelect!!!\", id);\n      selected.value = id;\n      store.commit('setSelected', id);\n      store.commit('set', name);\n      doClose();\n    };\n\n    const items = ref([{\n      id: 1,\n      name: 'Películas',\n      to: '/movies',\n      icon: 'arrow'\n    }, {\n      id: 2,\n      name: 'Actores',\n      to: '/movies',\n      icon: 'arrow'\n    }, {\n      id: 3,\n      name: 'Compañías',\n      to: '/movies',\n      icon: 'arrow'\n    }]);\n    return {\n      items,\n      doClose,\n      doSelect,\n      selected\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;AACXC,OAAK,GAAG;AAEJ,UAAMC,KAAI,GAAIF,QAAQ,EAAtB;AACA,UAAMG,QAAO,GAAIL,QAAQ,CAAC,MAAKI,KAAK,CAACE,KAAN,CAAYC,YAAlB,CAAzB;;AAEA,UAAMC,OAAM,GAAI,MAAM;AACpBJ,WAAK,CAACK,MAAN,CAAa,sBAAb,EAAqC,KAArC;AACF,KAFA;;AAIA,UAAMC,QAAO,GAAI,CAACC,EAAD,EAAKC,IAAL,KAAc;AAC7BC,aAAO,CAACC,GAAR,CAAY,aAAZ,EAA2BH,EAA3B;AACAN,cAAQ,CAACU,KAAT,GAAiBJ,EAAjB;AACAP,WAAK,CAACK,MAAN,CAAa,aAAb,EAA4BE,EAA5B;AACAP,WAAK,CAACK,MAAN,CAAa,KAAb,EAAoBG,IAApB;AACAJ,aAAO;AACT,KANA;;AAQA,UAAMQ,KAAI,GAAIf,GAAG,CAAC,CAChB;AACIU,QAAE,EAAC,CADP;AAEIC,UAAI,EAAC,WAFT;AAGIK,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KADgB,EAOhB;AACIP,QAAE,EAAC,CADP;AAEIC,UAAI,EAAC,SAFT;AAGIK,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KAPgB,EAahB;AACIP,QAAE,EAAC,CADP;AAEIC,UAAI,EAAC,WAFT;AAGIK,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KAbgB,CAAD,CAAjB;AAoBA,WAAO;AAAEF,WAAF;AAASR,aAAT;AAAkBE,cAAlB;AAA4BL;AAA5B,KAAP;AACH;;AAvCU,CAAf","names":["computed","ref","useStore","setup","store","selected","state","menuSelected","doClose","commit","doSelect","id","name","console","log","value","items","to","icon"],"sourceRoot":"","sources":["/Users/alberto/Workspace/atSistemas/prueba/src/components/SideMenu.vue"],"sourcesContent":["<template>\n  <div class=\"side-menu fixed h-[100vh] z-[100]\">\n    <va-sidebar v-for=\"item in items\" :key=\"item.id\" textColor=\"dark\">\n      <va-sidebar-item  class=\"pl-2\">\n        <va-sidebar-item-content>\n          <va-sidebar-item-title><span class='text-xl'>Menú</span></va-sidebar-item-title>\n          <va-icon name=\"close\" @click=\"doClose\"/>\n        </va-sidebar-item-content>\n      </va-sidebar-item>\n      <va-sidebar-item v-for=\"item in items\" :key=\"item.id\" class=\"pl-4 border-b-2\" :active=\"item.id === selected\">\n       <router-link :to='item.to' @click=\"doSelect(item.id, item.name)\"> \n          <va-sidebar-item-content>\n            <va-sidebar-item-title> {{item.name}} </va-sidebar-item-title>\n            <va-icon name=\"chevron_right\" />\n          </va-sidebar-item-content>\n       </router-link> \n      </va-sidebar-item>\n    </va-sidebar>\n\n  </div>\n</template>\n<script>\n\nimport { computed, ref } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default {\n    setup() {\n\n        const store = useStore();\n        const selected = computed(()=> store.state.menuSelected);\n\n        const doClose = () => {\n          store.commit('changeMenuVisibility', false)\n        }\n\n        const doSelect = (id, name) => {\n          console.log(\"doSelect!!!\", id)\n          selected.value = id;\n          store.commit('setSelected', id)\n          store.commit('set', name)\n          doClose()\n        }\n\n        const items = ref([\n          {\n              id:1,\n              name:'Películas',\n              to: '/movies',\n              icon:'arrow'\n          },\n          {\n              id:2,\n              name:'Actores',\n              to: '/movies',\n              icon:'arrow'\n          },\n          {\n              id:3,\n              name:'Compañías',\n              to: '/movies',\n              icon:'arrow'\n          }\n        ])\n        return { items, doClose, doSelect, selected }\n    },\n}\n</script>\n<style scoped>\n  .side-menu .va-sidebar__item__content {\n    padding: 15px;\n    text-align: left;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}