{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    const selected = ref(-1);\n    const store = useStore();\n\n    const doClose = () => {\n      store.commit('changeMenuVisibility', false);\n    };\n\n    const doSelect = id => {\n      console.log(\"doSelect!!!\", id);\n      selected.value = id;\n      store.commit('setSelected', id);\n      doClose();\n    };\n\n    const items = ref([{\n      id: 1,\n      name: 'Películas',\n      to: '/movies',\n      icon: 'arrow'\n    }, {\n      id: 2,\n      name: 'Actores',\n      to: '/movies',\n      icon: 'arrow'\n    }, {\n      id: 3,\n      name: 'Compañías',\n      to: '/movies',\n      icon: 'arrow'\n    }]);\n    return {\n      items,\n      doClose,\n      doSelect,\n      selected\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,SAASA,GAAT,QAAoB,KAApB;AACA,SAASC,QAAT,QAAyB,MAAzB;AAEA,eAAe;AACXC,OAAK,GAAG;AAEJ,UAAMC,QAAO,GAAIH,GAAG,CAAC,CAAC,CAAF,CAApB;AACA,UAAMI,KAAI,GAAIH,QAAQ,EAAtB;;AAEA,UAAMI,OAAM,GAAI,MAAM;AACpBD,WAAK,CAACE,MAAN,CAAa,sBAAb,EAAqC,KAArC;AACF,KAFA;;AAIA,UAAMC,QAAO,GAAKC,EAAD,IAAQ;AACvBC,aAAO,CAACC,GAAR,CAAY,aAAZ,EAA2BF,EAA3B;AACAL,cAAQ,CAACQ,KAAT,GAAiBH,EAAjB;AACAJ,WAAK,CAACE,MAAN,CAAa,aAAb,EAA4BE,EAA5B;AACAH,aAAO;AACT,KALA;;AAOA,UAAMO,KAAI,GAAIZ,GAAG,CAAC,CAChB;AACIQ,QAAE,EAAC,CADP;AAEIK,UAAI,EAAC,WAFT;AAGIC,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KADgB,EAOhB;AACIP,QAAE,EAAC,CADP;AAEIK,UAAI,EAAC,SAFT;AAGIC,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KAPgB,EAahB;AACIP,QAAE,EAAC,CADP;AAEIK,UAAI,EAAC,WAFT;AAGIC,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KAbgB,CAAD,CAAjB;AAoBA,WAAO;AAAEH,WAAF;AAASP,aAAT;AAAkBE,cAAlB;AAA4BJ;AAA5B,KAAP;AACH;;AAtCU,CAAf","names":["ref","useStore","setup","selected","store","doClose","commit","doSelect","id","console","log","value","items","name","to","icon"],"sourceRoot":"","sources":["/Users/alberto/Workspace/atSistemas/prueba/src/components/SideMenu.vue"],"sourcesContent":["<template>\n  <div class=\"side-menu fixed h-[100vh] z-[100]\">\n    <va-sidebar v-for=\"item in items\" :key=\"item.id\" textColor=\"dark\">\n      <va-sidebar-item  class=\"pl-2\">\n        <va-sidebar-item-content>\n          <va-sidebar-item-title><span class='text-xl'>Menú</span></va-sidebar-item-title>\n          <va-icon name=\"close\" @click=\"doClose\"/>\n        </va-sidebar-item-content>\n      </va-sidebar-item>\n      <va-sidebar-item v-for=\"item in items\" :key=\"item.id\" class=\"pl-4 border-b-2\" :active=\"item.id === selected\">\n       <router-link :to='item.to' @click=\"doSelect(item.id)\"> \n          <va-sidebar-item-content>\n            <va-sidebar-item-title> {{item.name}} </va-sidebar-item-title>\n            <va-icon name=\"chevron_right\" />\n          </va-sidebar-item-content>\n       </router-link> \n      </va-sidebar-item>\n    </va-sidebar>\n\n  </div>\n</template>\n<script>\n\nimport { ref } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default {\n    setup() {\n\n        const selected = ref(-1);\n        const store = useStore();\n\n        const doClose = () => {\n          store.commit('changeMenuVisibility', false)\n        }\n\n        const doSelect = (id) => {\n          console.log(\"doSelect!!!\", id)\n          selected.value = id;\n          store.commit('setSelected', id)\n          doClose()\n        }\n\n        const items = ref([\n          {\n              id:1,\n              name:'Películas',\n              to: '/movies',\n              icon:'arrow'\n          },\n          {\n              id:2,\n              name:'Actores',\n              to: '/movies',\n              icon:'arrow'\n          },\n          {\n              id:3,\n              name:'Compañías',\n              to: '/movies',\n              icon:'arrow'\n          }\n        ])\n        return { items, doClose, doSelect, selected }\n    },\n}\n</script>\n<style scoped>\n  .side-menu .va-sidebar__item__content {\n    padding: 15px;\n    text-align: left;\n  }\n</style>\n"