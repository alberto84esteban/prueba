{"ast":null,"code":"import { computed, ref } from 'vue';\nimport { useStore } from 'vuex';\nimport { useI18n } from 'vue-i18n';\nexport default {\n  setup() {\n    const {\n      t\n    } = useI18n();\n    const store = useStore();\n    const selected = computed(() => store.state.menuSelected);\n\n    const doClose = () => {\n      store.commit('changeMenuVisibility', false);\n    };\n\n    const doSelect = (id, name) => {\n      store.commit('setSelected', id);\n      store.commit('changeTitle', name);\n      doClose();\n    };\n\n    const items = ref([{\n      id: 1,\n      name: 'Películas',\n      to: '/movies',\n      icon: 'arrow'\n    }, {\n      id: 2,\n      name: 'Actores',\n      to: '/movies',\n      icon: 'arrow'\n    }, {\n      id: 3,\n      name: 'Compañías',\n      to: '/movies',\n      icon: 'arrow'\n    }]);\n    return {\n      items,\n      doClose,\n      doSelect,\n      selected,\n      t\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,SAASA,QAAT,EAAmBC,GAAnB,QAA8B,KAA9B;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,SAASC,OAAT,QAAwB,UAAxB;AAEA,eAAe;AACXC,OAAK,GAAG;AAEJ,UAAM;AAAEC;AAAF,QAAQF,OAAO,EAArB;AACA,UAAMG,KAAI,GAAIJ,QAAQ,EAAtB;AACA,UAAMK,QAAO,GAAIP,QAAQ,CAAC,MAAKM,KAAK,CAACE,KAAN,CAAYC,YAAlB,CAAzB;;AAEA,UAAMC,OAAM,GAAI,MAAM;AACpBJ,WAAK,CAACK,MAAN,CAAa,sBAAb,EAAqC,KAArC;AACF,KAFA;;AAIA,UAAMC,QAAO,GAAI,CAACC,EAAD,EAAKC,IAAL,KAAc;AAC7BR,WAAK,CAACK,MAAN,CAAa,aAAb,EAA4BE,EAA5B;AACAP,WAAK,CAACK,MAAN,CAAa,aAAb,EAA4BG,IAA5B;AACAJ,aAAO;AACT,KAJA;;AAMA,UAAMK,KAAI,GAAId,GAAG,CAAC,CAChB;AACIY,QAAE,EAAC,CADP;AAEIC,UAAI,EAAC,WAFT;AAGIE,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KADgB,EAOhB;AACIJ,QAAE,EAAC,CADP;AAEIC,UAAI,EAAC,SAFT;AAGIE,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KAPgB,EAahB;AACIJ,QAAE,EAAC,CADP;AAEIC,UAAI,EAAC,WAFT;AAGIE,QAAE,EAAE,SAHR;AAIIC,UAAI,EAAC;AAJT,KAbgB,CAAD,CAAjB;AAoBA,WAAO;AAAEF,WAAF;AAASL,aAAT;AAAkBE,cAAlB;AAA4BL,cAA5B;AAAsCF;AAAtC,KAAP;AACH;;AAtCU,CAAf","names":["computed","ref","useStore","useI18n","setup","t","store","selected","state","menuSelected","doClose","commit","doSelect","id","name","items","to","icon"],"sourceRoot":"","sources":["/Users/alberto/Workspace/atSistemas/prueba/src/components/SideMenu.vue"],"sourcesContent":["<template>\n  <div class=\"side-menu fixed h-[100vh] z-[100]\">\n    <va-sidebar v-for=\"item in items\" :key=\"item.id\" textColor=\"dark\">\n      <va-sidebar-item  class=\"pl-2\">\n        <va-sidebar-item-content>\n          <va-sidebar-item-title><span class='text-xl'>{{ $t(\"menu\") }}</span></va-sidebar-item-title>\n          <va-icon name=\"close\" @click=\"doClose\"/>\n        </va-sidebar-item-content>\n      </va-sidebar-item>\n      <va-sidebar-item v-for=\"item in items\" :key=\"item.id\" class=\"pl-4 border-b-2\" :active=\"item.id === selected\">\n       <router-link :to='item.to' @click=\"doSelect(item.id, item.name)\"> \n          <va-sidebar-item-content>\n            <va-sidebar-item-title> {{item.name}} </va-sidebar-item-title>\n            <va-icon name=\"chevron_right\" />\n          </va-sidebar-item-content>\n       </router-link> \n      </va-sidebar-item>\n    </va-sidebar>\n\n  </div>\n</template>\n<script>\n\nimport { computed, ref } from 'vue'\nimport { useStore } from 'vuex'\nimport { useI18n } from 'vue-i18n'\n\nexport default {\n    setup() {\n\n        const { t } = useI18n();\n        const store = useStore();\n        const selected = computed(()=> store.state.menuSelected);\n\n        const doClose = () => {\n          store.commit('changeMenuVisibility', false)\n        }\n\n        const doSelect = (id, name) => {\n          store.commit('setSelected', id)\n          store.commit('changeTitle', name)\n          doClose()\n        }\n\n        const items = ref([\n          {\n              id:1,\n              name:'Películas',\n              to: '/movies',\n              icon:'arrow'\n          },\n          {\n              id:2,\n              name:'Actores',\n              to: '/movies',\n              icon:'arrow'\n          },\n          {\n              id:3,\n              name:'Compañías',\n              to: '/movies',\n              icon:'arrow'\n          }\n        ])\n        return { items, doClose, doSelect, selected, t }\n    },\n}\n</script>\n<style scoped>\n  .side-menu .va-sidebar__item__content {\n    padding: 15px;\n    text-align: left;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}